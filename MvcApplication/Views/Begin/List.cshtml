@using MvcApplication.Models;
@model IEnumerable<Member>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table class="table">
    <tr>
        <td>氏名</td>
        <td>メールアドレス</td>
        <td>誕生日</td>
        <td>既婚</td>
        <td>備考</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Birth</td>
            <td>@item.Married</td>
            <td>@item.Memo</td>
        </tr>
    }
</table>
